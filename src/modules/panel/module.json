{
    "name": "caweb/panel",
    "d4Shortcode": "et_pb_ca_panel",
    "title": "Panel Module",
    "titles": "CAWeb Modules",
    "category": "module",
    "moduleIcon": "caweb/caweb",
    "moduleClassName": "et_pb_ca_panel",
    "moduleOrderClassName": "et_pb_ca_panel",
    "attributes":{
      "module": {
        "type": "object",
        "selector": "{{selector}}",
        "settings": {
          "advanced": {
            "htmlAttributes": {},
            "layout": {
              "groupType": "group-item",
              "item": {
                "groupSlug": "panelLayout",
                "priority": 1,
                "render": true,
                "attrName": "module.advanced.layout",
                "subName": "style",
                "label": "Style",
                "description": "Here you can choose the style of panel to display.",
                "features": {
                  "dynamicContent": false
                },
                "component": {
                  "name": "divi/select",
                  "type": "field",
                  "props": {
                      "defaultValue": "default",
                      "options": {
                          "none": {
                            "label": "None",
                            "value": "none"
                          },
                          "default": {
                            "value": "default",
                            "label": "Default"
                          },
                          "standout": {
                            "value": "standout",
                            "label": "Standout"
                          },
                          "standout highlight": {
                            "value": "standout highlight",
                            "label": "Standout Highlight"
                          },
                          "overstated": {
                            "value": "overstated",
                            "label": "Overstated"
                          },
                          "understated": {
                            "value": "understated",
                            "label": "Understated"
                          }
                        } 
                    } 
                }
              }
            }
          }
        }
      },
      "title": {
        "type": "object",
        "selector": "{{selector}} .card-header",
        "attrName": "title",
        "attributes": {
          "class": "card-title pb-0"
        },
        "inlineEditor": "plainText",
        "elementType": "heading",
        "childrenSanitizer": "et_core_esc_previously",
        "settings": {
          "innerContent": {
            "groupType": "group-items",
            "items": {
              "title": {
                "groupSlug": "panelHeader",
                "priority": 10,
                "label": "Heading",
                "description": "Here you can enter a Heading Title.",
                "attrName": "title.innerContent",
                "render": true,
                "features": {
                  "dynamicContent": false
                },
                "component": {
                    "name": "divi/text",
                    "type": "field"
                }
              },
              "enable": {
                "groupSlug": "panelHeader",
                "priority": 10,
                "render": true,
                "attrName": "title.readMore.button",
                "subName": "enable",
                "label": "Read More Button",
                "description": "Here you can choose to display a button.",
                "features": {
                  "dynamicContent": false
                },
                "component": {
                  "name": "divi/toggle",
                  "type": "field"
                }
              },
              "url": {
                "groupSlug": "panelHeader",
                "priority": 10,
                "render": true,
                "attrName": "title.readMore.link",
                "subName": "url",
                "label": "Read More Button URL",
                "description": "Here you can enter the URL for the button.",
                "features": {
                  "dynamicContent": false
                },
                "component": {
                  "name": "divi/text",
                  "type": "field"
                }
              },
              "text": {
                "groupSlug": "panelHeader",
                "priority": 10,
                "render": true,
                "attrName": "title.readMore.link",
                "subName": "text",
                "label": "Read More Button Text",
                "description": "Here you can enter the text for the button.",
                "features": {
                  "dynamicContent": false
                },
                "component": {
                  "name": "divi/text",
                  "type": "field"
                }
              },
              "target":{
                "groupSlug": "panelHeader",
                "priority": 10,
                "render": true,
                "attrName": "title.readMore.link",
                "subName": "target",
                "label": "Read More Button Target",
                "description": "Here you can choose whether or not your link opens in a new window.",
                "features": {
                  "dynamicContent": false
                },
                "component": {
                  "name": "divi/select",
                  "type": "field",
                  "props": {
                    "defaultValue": "on",
                    "options": {
                      "off": {
                        "value": "off",
                        "label": "In The Current Tab"
                      },
                      "on": {
                        "value": "on",
                        "label": "In A New Tab"
                      }
                    } 
                  }
                }
              }
            }
          },
          "decoration": {
            "font": {
              "component": {
                "props": {
                  "groupLabel": "Header",
                  "fields": {
                    "textAlign": {
                      "label": "Heading Alignment",
                      "priority": 1
                    },
                    "headingLevel": {
                      "label": "Heading Level",
                      "priority": 2
                    },
                    "color": {
                      "label": "Heading Color",
                      "priority": 3
                    },
                    "showIcon": {
                      "attrName": "title.readMore.icon",
                      "priority": 4,
                      "subName": "show",
                      "render": true,
                      "label": "Use Icon",
                      "description": "Choose whether to display an icon before the Heading.",
                      "features": {
                        "dynamicContent": false
                      },
                      "component": {
                        "name": "divi/toggle",
                        "type": "field"
                      }
                    },
                    "icon": {
                      "attrName": "title.readMore.icon",
                      "priority": 4,
                      "render": true,
                      "label": "Icon",
                      "description": "Here you can select a Heading Icon.",
                      "features": {
                        "dynamicContent": false
                      },
                      "component": {
                        "name": "divi/icon-picker",
                        "type": "field"
                      }
                    },
                    "family": {
                      "render": false
                    },
                    "weight": {
                      "render": false
                    },
                    "style": {
                      "render": false
                    },
                    "size": {
                      "render": false
                    },
                    "letterSpacing": {
                      "render": false
                    },
                    "lineHeight": {
                      "render": false
                    },
                    "textShadowGroup": {
                      "render": false
                    }
                  }
                }
              }
            }
          }
        }       
      },
      "content": {
        "type": "object",
        "selector": "{{selector}} .card-body",
        "attributes": {
          "class": "card-body"
        },
        "tagName": "div",
        "inlineEditor": "richText",
        "childrenSanitizer": "et_core_esc_previously",
        "allowHtml": true,
        "settings": {
          "innerContent": {
            "groupType": "group-item",
            "item": {
              "groupSlug": "panelBody",
              "priority": 20,
              "render": true,
              "attrName": "content.innerContent",
              "label": "Content",
              "description": "Content",
              "features": {
                "dynamicContent": false
              },
              "component": {
                "name": "divi/richtext",
                "type": "field"
              }
            }
          }
        }
      }
    },
    "settings": {
      "groups": {
        "panelBody": {
          "panel": "content",
          "priority": 2,
          "groupName": "panelBody",
          "component": {
            "name": "divi/composite",
            "props": {
              "groupLabel": "Body"
            }
          }
        },
        "panelHeader": {
          "groupName": "panelHeader",
          "panel": "content",
          "priority": 1,
          "component": {
            "name": "divi/composite",
            "props": {
              "groupLabel": "Header"
            }
          }
        },
        "panelLayout": {
          "panel": "design",
          "priority": 1,
          "groupName": "panelLayout",
          "component": {
            "name": "divi/composite",
            "props": {
              "groupLabel": "Style"
            }
          }
        }
      }
    }
}
  